{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "HistoricalQuote",
    "description": "Historical stock prices and volumes",
    "type": "object",
    "properties": {
        "symbol": {
            "type": "string",
            "description": "Stock ticker symbol (e.g., AAPL, MSFT)",
            "pattern": "^[A-Z]{1,5}$"
        },
        "date": {
            "type": "string",
            "description": "Trading date in MM/DD/YYYY format",
            "pattern": "^(0[1-9]|1[0-2])\\/(0[1-9]|[12][0-9]|3[01])\\/\\d{4}$"
        },
        "close_last": {
            "type": "string",
            "description": "Closing price (string format with $ prefix)",
            "pattern": "^\\$\\d{1,4}\\.\\d{2}$"
        },
        "volume": {
            "type": "integer",
            "description": "Trading volume (number of shares traded)"
        },
        "open": {
            "type": "string",
            "description": "Opening price (string format with $ prefix)",
            "pattern": "^\\$\\d{1,4}\\.\\d{2}$"
        },
        "high": {
            "type": "string",
            "description": "Highest price (string format with $ prefix)",
            "pattern": "^\\$\\d{1,4}\\.\\d{2}$"
        },
        "low": {
            "type": "string",
            "description": "Lowest price (string format with $ prefix)",
            "pattern": "^\\$\\d{1,4}\\.\\d{2}$"
        }
    },
    "required": [
        "symbol",
        "date",
        "close_last",
        "volume",
        "open",
        "high",
        "low"
    ]
}